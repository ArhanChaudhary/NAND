/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
    And(a=nota, b=b, out=caseone);
    Not(in=b, out=notb);
    And(a=a, b=notb, out=casetwo);
    Or(a=caseone, b=casetwo, out=out);
}